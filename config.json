{
  "PhoneAsItem": {
    "enabled": true,
    "exportResource": "noire_assets",
    "exportFunction": "hasPhone"
  },
  "general": {
    "useResourceIntegration": true,
    "toggleKey": "f1",
    "toggleCommand": "phone",
    "defaultLanguage": "en"
  },
  "profanityFilter": {
    "enabled": false,
    "badWords": ["esx"]
  },
  "database": {
    "useIdentifierPrefix": false,
    "playerTable": "users",
    "identifierColumn": "identifier",
    "identifierType": "steam",
    "profileQueries": true,
    "phoneNumberColumn": "phone_number"
  },
  "images": {
    "url": "https://api.imgur.com/3/image",
    "type": "imgur",
    "imageEncoding": "jpg",
    "contentType": "multipart/form-data",
    "useContentType": true,
    "authorizationHeader": "Authorization",
    "authorizationPrefix": "Client-ID",
    "useAuthorization": true,
    "returnedDataIndexes": ["data", "link"]
  },
  "twitter": {
    "showNotifications": true,
    "generateProfileNameFromUsers": false,
    "allowEditableProfileName": true,
    "allowDeleteTweets": false,
    "allowReportTweets": true,
    "allowRetweet": true,
    "characterLimit": 160,
    "newLineLimit": 10,
    "enableAvatars": true,
    "enableEmojis": true,
    "enableImages": true,
    "maxImages": 1,
    "badWords": [
      "nigger",
      "comserv",
      "coms",
      "admin",
      "gods",
      "bobby",
      "staff",
      "ooc",
      "sauce",
      "cheat",
      "hack",
      "metagam",
      "rdm",
      "vdm",
      "report",
      "ticket"
    ]
  },
  "match": {
    "generateProfileNameFromUsers": false,
    "allowEditableProfileName": true
  },
  "marketplace": {
    "persistListings": true
  },
  "debug": {
    "level": "error",
    "enabled": true,
    "sentryEnabled": true
  },
  "defaultContacts": [
    {
      "id": "911",
      "avatar": "default.jpg",
      "display": "911",
      "number": 911
    }
  ],
  "disabledApps": ["BROWSER"],
  "apps": ["pefcl"],
  "voiceMessage": {
    "enabled": false,
    "authorizationHeader": "PE-Secret",
    "url": "",
    "returnedDataIndexes": ["url"]
  }
}
